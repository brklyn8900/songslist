<h1>This is the EDIT page for <%= params[:id] %></h1>

<div class="form">
  <%= form_for @song do |f| %>
    <%= f.label :title %><br/>
    <%= f.text_field :title %><br/>

    <%= f.label :artist %><br/>
    <%= f.text_field :artist %><br/>

    <%= f.label :url %><br/>
    <%= f.text_field :url %><br/>

    <%= f.submit %>
  <% end %>
</div>
